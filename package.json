{
  "name": "fork-cms-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Opiniated Fork CMS theme boilerplate with Tailwind, React, ESNext, Post-CSS and Hot Module Replacement support using Webpack 4",
  "repository": {
    "type": "git",
    "url": "git@github.com:jessedobbelaere/fork-cms-webpack-boilerplate.git"
  },
  "main": "Core/Js/app.ts",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --env.mode=development",
    "build:prod": "webpack --env.mode=production",
    "start:dev": "webpack-dev-server --env.mode=development --hot",
    "watch": "npm run start:dev",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "prettier-check": "prettier --list-different '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix Core/Js/*"
  },
  "dependencies": {
    "core-js": "3.4.1",
    "quicklink": "2.0.0-alpha",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "turbolinks": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@babel/preset-typescript": "7.7.2",
    "@hot-loader/react-dom": "16.10.2",
    "@testing-library/react": "9.3.2",
    "@types/jest": "24.0.22",
    "@types/packery": "1.4.31",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/turbolinks": "5.2.0",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "chokidar": "3.3.0",
    "clean-css": "4.2.1",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.2.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.2.0",
    "glob-all": "3.1.0",
    "imagemin": "7.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "7.1.0",
    "imagemin-svgo": "7.0.0",
    "img-loader": "3.0.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "mini-css-extract-plugin": "0.8.0",
    "postcss-clean": "1.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "postcss-simple-vars": "5.0.2",
    "prettier": "1.19.1",
    "purgecss-webpack-plugin": "1.6.0",
    "raw-loader": "3.1.0",
    "react-hot-loader": "4.12.16",
    "react-test-renderer": "16.11.0",
    "size-plugin": "2.0.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.0",
    "tailwindcss": "1.1.3",
    "terser-webpack-plugin": "2.1.3",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2",
    "url-loader": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "4.2.2"
  }
}
