{
  "name": "fork-cms-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Fork CMS theme boilerplate with ES2017, Sass and Hot Module Replacement support using Webpack 4",
  "repository": {
    "type": "git",
    "url": "git@github.com:jessedobbelaere/fork-cms-webpack-boilerplate.git"
  },
  "main": "Core/Js/app.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --env.mode=development",
    "build:prod": "webpack --env.mode=production",
    "start:dev": "webpack-dev-server --env.mode=development --hot",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "prettier-check": "prettier --list-different '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "test": "echo \"Error: no tests implemented yet\" && exit 0"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.0.1",
    "file-loader": "^3.0.1",
    "imagemin": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-clean": "^1.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "1.16.4",
    "sass-loader": "^7.1.0",
    "size-plugin": "^1.1.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  }
}
