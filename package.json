{
  "name": "fork-cms-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Fork CMS theme boilerplate with ES2017, Sass and Hot Module Replacement support using Webpack 4",
  "repository": {
    "type": "git",
    "url": "git@github.com:jessedobbelaere/fork-cms-webpack-boilerplate.git"
  },
  "main": "Core/Js/app.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --env.mode=development",
    "build:prod": "webpack --env.mode=production",
    "start:dev": "webpack-dev-server --env.mode=development --hot",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "prettier-check": "prettier --list-different '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "test": "echo \"Error: no tests implemented yet\" && exit 0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.3.0",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.5.0",
    "css-loader": "^3.3.2",
    "file-loader": "^5.0.2",
    "glob-all": "^3.1.0",
    "imagemin": "^6.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.0.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-clean": "^1.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "1.19.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "sass-loader": "^7.3.1",
    "size-plugin": "^2.0.1",
    "style-loader": "^1.0.1",
    "tailwindcss": "^1.1.4",
    "terser-webpack-plugin": "^2.2.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
}
